{
    "name": "es-pack-js",
    "version": "0.3.0",
    "description": "A webpack-based tool for building JavaScript module variants",
    "bin": {
        "es-pack": "bin/es-pack"
    },
    "main": "src/index.js",
    "directories": {},
    "scripts": {
        "test": "npm run test:node && npm run test:browser && npm run test:verify",
        "test:node": "jest tests/node/main.test.js",
        "test:browser": "jest -c jest.config.browser.js tests/browser/main.test.js",
        "test:verify": "bin/es-pack build examples/test -m umd esm esm-compat --verify",
        "test:node:sf": "jest tests/node/main.test.js --silent false"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/w3reality/es-pack-js.git"
    },
    "keywords": [
        "webpack",
        "module",
        "build",
        "umd",
        "esm"
    ],
    "author": "j-devel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/w3reality/es-pack-js/issues"
    },
    "homepage": "https://github.com/w3reality/es-pack-js",
    "dependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-add-module-exports": "^0.2.1",
        "eslint": "^6.2.2",
        "eslint-loader": "^3.0.0",
        "express": "^4.17.1",
        "fs-extra": "^8.1.0",
        "jest": "^25.1.0",
        "jest-puppeteer": "^4.4.0",
        "puppeteer": "^2.1.1",
        "terser-webpack-plugin": "^2.2.2",
        "toml": "^3.0.0",
        "webpack": "^4.41.2",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.9",
        "webpack-var2esm-plugin": "^1.2.1",
        "yargs": "^15.0.2"
    }
}
